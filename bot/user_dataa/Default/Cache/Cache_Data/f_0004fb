        <ul class="dropdown-menu dropdown-menu-left" role="menu" style="left: auto; right: 0%;"><li ng-repeat="i in o.pageNumbers">
          <a href ng-click="changePage(i)">
            <span ng-style="styleForCurrentPage(i)">{{i}}</span>
          </a>
        </li></ul>
      </div>
      <button type="button" class="btn btn-default" ng-click="nextPage()" ng-disabled="!hasNextPage()" tabindex=-1><i class="fa fa-forward"></i></button>
      <button type="button" class="btn btn-default" ng-click="lastPage()" ng-disabled="!hasNextPage()" tabindex=-1><i class="fa fa-fast-forward"></i></button>
      <div class="btn-group" ng-if="o.hasColumnList">
