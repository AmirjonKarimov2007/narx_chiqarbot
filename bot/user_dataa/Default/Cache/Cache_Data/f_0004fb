  <script type="text/ng-template" id="b-pagination.html">
  <div class="input-group">
    <input type="text" ng-style="{'border-top-left-radius': (o.small ? '3px' : '4px'), 'border-bottom-left-radius': (o.small ? '3px' : '4px')}" class="form-control" ng-class="{'input-sm': o.small}"
           placeholder="{{o.bConfig.langs.search_for}}" ng-keypress="onSearchKeyPress($event)" ng-model="o.searchValue" ng-blur="onSearchBlur()" ng-if="o.hasSearch"/>
    <div class="input-group-btn">
      <div class="btn-group" ng-if="o.hasFilter">
        <button type="button" class="btn btn-default" ng-class="{'btn-warning': !o.isViewAll}" ng-click="openFilter()" tabindex=-1><i class="fa fa-filter"></i></button>
      </div>
      <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" tabindex=-1>
          {{o.rowPerPageTitle}} <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" style="left: auto; right: 0%;"><li ng-repeat="i in o.limits">
          <a href ng-click="changeLimit(i)">
            <span ng-style="styleForCurrentLimit(i)">{{i}}</span>
          </a>
        </li></ul>
      </div>
      <button type="button" class="btn btn-default" ng-click="firstPage()" ng-disabled="!hasPrevPage()" tabindex=-1><i class="fa fa-fast-backward"></i></button>
      <button type="button" class="btn btn-default" ng-click="prevPage()" ng-disabled="!hasPrevPage()" tabindex=-1><i class="fa fa-backward"></i></button>
      <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" tabindex=-1>
          {{o.currentPage}} <span class="caret"></span>
